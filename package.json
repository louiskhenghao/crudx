    "@mui/material": "^5.14.18",
    "@nx/next": "16.6.0",
    "@webbyx/mui": "^0.0.24",
    "@webbyx/next-js": "^0.0.2",
    "animate.css": "^4.1.1",
    "classnames": "^2.3.2",
    "currency-symbol-map": "^5.1.0",
    "graphql": "^16.8.1",
    "isomorphic-unfetch": "^4.0.2",
    "lodash": "^4.17.21",
    "next": "13.4.1",
    "next-translate": "^2.5.3",
    "nookies": "^2.5.2",
    "numeral": "^2.0.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-is": "^18.2.0",
    "react-perfect-scrollbar": "^1.5.8",
    "react-use": "^17.4.0",
    "stylis": "^4.3.0",
    "stylis-plugin-rtl": "^2.1.1",
    "tslib": "^2.3.0",
    "zustand": "^4.4.1"
